{
  "buildCommand": "npm run build && (cd docsite && npm ci && npm run build)",
  "rewrites": [
    {
      "source": "/feed/",
      "destination": "/feed.xml"
    },
    {
      "source": "/sitemap/",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots",
      "destination": "/robots.txt"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}